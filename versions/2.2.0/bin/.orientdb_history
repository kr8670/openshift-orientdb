connect remote:localhost root admin
help create database
create database remote:localhost/common root admin plocal
alter database set date_format=dd.MM.yyyy
alter database set dateformat=dd.MM.yyyy
alter database set DATEFORMAT 'dd.MM.yyyy'
alter database set DATEFORMAT='dd.MM.yyyy'
alter database DATEFORMAT='dd.MM.yyyy'
info
alter database DATETIMEFORMAT='dd.MM.yyyy HH:mm:ss'
commit
connect remote:localhost/common admin admin
exit
